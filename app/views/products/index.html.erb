<h1>Awesome Products</h1>
<% @products.each do |product| %>
  <div class="product">
    <div class='product-upvote'>
      <div class="product-arrow"></div>
      <div class='product-count'>0</div>
    </div>
    <div class='product-body'>
      <h3><%= product.name %></h3>
      <p><%= product.tagline  %></p>
    </div>
  </div>
<% end %>